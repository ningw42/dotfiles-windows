    "keybindings": 
    [
        {
            "id": "Terminal.Quit",
            "keys": "ctrl+shift+q"
        },
        {
            "id": "Terminal.ClosePane",
            "keys": "ctrl+shift+w"
        },
        {
            "id": "Terminal.OpenNewTab",
            "keys": "ctrl+shift+t"
        },
        {
            "id": "Terminal.PrevTab",
            "keys": "ctrl+shift+h"
        },
        {
            "id": "Terminal.NextTab",
            "keys": "ctrl+shift+l"
        },
        {
            "id": "Terminal.CopyToClipboard",
            "keys": "ctrl+shift+c"
        },
        {
            "id": "Terminal.PasteFromClipboard",
            "keys": "ctrl+shift+v"
        }
    ],
